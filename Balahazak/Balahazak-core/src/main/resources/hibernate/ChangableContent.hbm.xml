<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="hb.ChangableContent" table="changable_content">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		<property column="page_id" length="11" name="pageId" type="java.lang.Integer"/>
		<property column="content" length="40000" name="content" type="java.lang.String"/>
		<property column="last_modify" length="18" name="lastModify" type="java.sql.Timestamp"/>
	</class>
</hibernate-mapping>
