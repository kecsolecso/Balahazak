<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="hb.PHPReplication" table="php_replications">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>
		<property column="name" length="128" name="name" type="java.lang.String"/>
		<property column="web_url" length="256" name="webURL" type="java.lang.String"/>
		<property column="db_path" length="256" name="dbPath" type="java.lang.String"/>
	</class>
</hibernate-mapping>
